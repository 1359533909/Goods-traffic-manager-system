<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.mxl.demo.mapper.CompanyUserMapper">
	<insert id="insertCompanyUser" parameterType="CompanyUser">
		INSERT INTO companyuser VALUE (NULL,#{username},#{password},NULL,#{companyname},#{principal},#{moneydeposit},#{companyaddress})
	</insert>
</mapper>